<header class="header">
  <% if (locals.user) { %> <% if(locals.user.isAdmin) { %>
  <a href="/" class="header__link header__link--1">
    <img src="/img/logo.png" alt="header logo" class="header__logo" />
  </a>

  <a href="/admin/assignReview" class="header__link header__link--3">Review</a>

  <a href="/admin" class="header__link header__link--4">Dashboard</a>

  <a
    href="/auth/logout"
    class="header__link header__link--5 header__link--logout"
    ><%= locals.user.name %>&mdash;Logout</a
  >
  <% } else { %>
  <a href="/" class="header__link header__link--1">
    <img src="/img/logo.png" alt="header logo" class="header__logo" />
  </a>

  <a href="/employee/feedback" class="header__link header__link--3">Feedback</a>

  <a href="/employee" class="header__link header__link--4">Dashboard</a>

  <a
    href="/auth/logout"
    class="header__link header__link--5 header__link--logout"
    ><%= locals.user.name %>&mdash;Logout</a
  >
  <% } %> <% } else { %>
  <a href="/" class="header__link header__link--1">
    <img src="/img/logo.png" alt="header logo" class="header__logo" />
  </a>
  <a href="/auth/login" class="header__link header__link--4">Signin</a>

  <a href="/auth/register" class="header__link header__link--5">Signup</a>

  <% } %>
</header>
